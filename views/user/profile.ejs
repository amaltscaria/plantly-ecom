    function addAddress() {
        event.preventDefault();

        // Reset error messages
        $('#user--message-address').css('display', 'none');
        $('.invalid-feedback').css('display', 'none');
        
        // Validate and get form values
        const fullName = $('#addressUser').val().trim();
        const addressLine1 = $('#address_line1').val().trim();
        const city = $('#city').val().trim();
        const state = $('#state').val().trim();
        const postCode = $('#post_code').val().trim();
        const country = $('#country').val().trim();
        const mobileNumber = $('#mobile_number').val().trim();
        const isDefault = $('#default_address').is(':checked');

        // Perform individual validations
        let isValid = true;

        if (!fullName) {
            $('#addressUser + .invalid-feedback').text('Please enter your full name.').css('display', 'block');
            isValid = false;
        }

        if (!addressLine1) {
            $('#address_line1 + .invalid-feedback').text('Please enter your address.').css('display', 'block');
            isValid = false;
        }

        if (!city) {
            $('#city + .invalid-feedback').text('Please enter your city.').css('display', 'block');
            isValid = false;
        }

        if (!state) {
            $('#state + .invalid-feedback').text('Please enter your state.').css('display', 'block');
            isValid = false;
        }

        if (!postCode || !/^\d{6}$/.test(postCode)) {
            $('#post_code + .invalid-feedback').text('Please enter a valid 6-digit post code.').css('display', 'block');
            isValid = false;
        }

        if (!country) {
            $('#country + .invalid-feedback').text('Please enter your country.').css('display', 'block');
            isValid = false;
        }

        if (!mobileNumber || !/^\d{10}$/.test(mobileNumber)) {
            $('#mobile_number + .invalid-feedback').text('Please enter a valid 10-digit mobile number.').css('display', 'block');
            isValid = false;
        }

        if (isValid) {
            // Proceed with form submission or API call
            const address = {
                fullName,
                addressLine1,
                city,
                state,
                postCode,
                country,
                mobileNumber,
                isDefault
            };

             // If all validations pass, proceed with the fetch request
        fetch('/addAddress', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json',
