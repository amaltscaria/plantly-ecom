    // Get input values
    const currentPassword = $('#currentPassword').val();
    const newPassword = $('#password').val();
    const confirmNewPassword = $('#confirmPassword').val();
    console.log(currentPassword);
    console.log(newPassword);
    console.log(confirmNewPassword);

    let isValid = true;

    // Perform validations
    if (currentPassword === '') {
      $('#currentPasswordValidation').text('Current password is required');
      isValid = false;
    }

    if (newPassword === '') {
      $('#passwordValidation').text('New password is required');
      isValid = false;
    }
    if (newPassword.length < 6) {
      $('#passwordValidation').text('New password must be atleast 6 characters');
      isValid = false;
    }

    if (confirmNewPassword === '') {
      $('#confirmPasswordValidation').text('Confirm password is required');
      isValid = false;
    }

    if (newPassword !== confirmNewPassword) {
      $('#confirmPasswordValidation').text('Passwords do not match');
      isValid = false;
    }
    if (newPassword === currentPassword) {
      $('#passwordValidation').text('New Password Cannot be same as Current Password');
      isValid = false;
    }

    if (!isValid) return;

    // Your fetch request
    fetch('/resetPassword', {
      method: 'PATCH',
      headers: {
        'Content-Type': 'application/json',
      },
      body: JSON.stringify({
        currentPassword,
        newPassword,
