      border: 1px solid #ddd;
      padding: 8px;
      margin-bottom: 10px;
      width: 100%;
      box-sizing: border-box;
    }

    .profile-field:disabled {
      background-color: #f5f5f5;
    }

    .user-message {
      width: 50%;
    }

    .input-border {
      border: 1px solid #e2e2e2;
    }

    #wallet {
      font-family: 'Helvetica Neue', Arial, sans-serif;
      background-color: #f4f4f4;
      display: flex;
      align-items: center;
      justify-content: center;
      height: 80vh;
      margin: 0;
      margin-left: 120px;
    }

    .wallet {
      background-color: #fff;
      box-shadow: 0 0 20px rgba(0, 0, 0, 0.1);
      border-radius: 10px;
      overflow: hidden;
      max-width: 400px;
      width: 100%;
      text-align: center;
      padding: 20px;
      transition: transform 0.3s ease-in-out;
      margin-left: 25%;
    }

    .wallet:hover {
      transform: scale(1.05);
    }

    .wallet-header {
      background-color: #6ea820;
      color: #fff;
      padding: 1rem;
      border-radius: 10px 10px 0 0;
    }

    .wallet-image {
      max-width: 100%;
      height: 200px;
      border-radius: 50%;
      margin: 20px 0;
    }

    .wallet-balance {
      font-size: 28px;
      font-weight: bold;
      margin: 20px 0;
      color: #333;
      font-family: 'Times New Roman', Times, serif;
    }

    .wallet-actions button {
      background-color: #6ea820;
      color: #fff;
      padding: 10px 20px;
      border: none;
      border-radius: 8px;
      cursor: pointer;
      transition: background-color 0.3s ease;
      font-size: 16px;
      margin: 0 10px;
    }

    .wallet-actions button:hover {
      background-color: #45a049;
    }

    .transaction-history {
      padding: 20px;
      text-align: left;
    }

    .transaction-history h2 {
      margin-top: 5px;
      color: #7c7c7c;
      margin-bottom: 10px;
      font-size: 15px;
    }

    .transaction-list {
      list-style-type: none;
      padding: 0;
      margin: 0;
