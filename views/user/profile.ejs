    try {
      const link = $('#referralLink').val();
      await navigator.clipboard.writeText(link);
      $('#copyBtn').text('copied');
    setTimeout(()=>{
      $('#copyBtn').text('copy');
    },3000)
    } catch (err) {
      console.error('Failed to copy: ', err);
    }
  }

