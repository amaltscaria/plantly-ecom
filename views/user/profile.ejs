   <script>
    // Show user details section by default
    document.addEventListener('DOMContentLoaded', function () {
        showSection('user-details');
    });

    function showSection(sectionId) {
        // Hide all sections
        const sections = document.querySelectorAll('.user-content');
        sections.forEach(section => {
            section.style.display = 'none';
        });

        // Remove the 'active' class from all navigation items
        const navItems = document.querySelectorAll('.user-navbar a');
        navItems.forEach(item => {
            item.classList.remove('active', 'hover');
        });

        // Show the selected section
        const selectedSection = document.getElementById(sectionId);
        if (selectedSection) {
            selectedSection.style.display = 'block';

            // Add 'active' class to the clicked navigation item
            const clickedNavItem = document.querySelector(`.user-navbar a[href="#"][onclick="showSection('${sectionId}')"]`);
            if (clickedNavItem) {
                clickedNavItem.classList.add('active', 'hover');
            }
        }
    }

    // Add event listeners to retain hover effect until another section is selected
    const navItems = document.querySelectorAll('.user-navbar a');
    navItems.forEach(item => {
        item.addEventListener('mouseover', function () {
            item.classList.add('hover');
        });

        item.addEventListener('mouseout', function () {
            if (!item.classList.contains('active')) {
                item.classList.remove('hover');
            }
        });
    });

    function editProfile() {
    const editProfileBtn = $('#edit-profile-btn');
    const inputFields = $('.profile-field');

    // Check if the button is in "Edit Profile" state
    if (editProfileBtn.text() === 'Edit Profile') {
        editProfileBtn.text('Save Changes');
        $('#edit-profile').find('.profile-field').css('color', '#3d4bdb');
        // Display a message in the email field
        $('#edit_email').val('To change email, please contact support at plantlyecom@gmail.com');
        $('#edit-profile').find('.profile-email').css('color', 'green');
        // Enable input fields for editing, except for the email field
        $('#edit-profile').find('.profile-field').not('#edit_email').prop('disabled', false);
        $('#edit_email').prop('disabled', true);
    } else {
            //Reset the error messages
             $('#user-error-message').css('display', 'none');
             $('#user-message').css('display', 'none');
             $('#error-edit_name').text('');
             $('#error-edit_last_name').text('')
             $('#error-edit_phone').text('');

             // Save Changes logic using fetch
             const updatedFirstName = $('#edit_name').val();
             const updatedLastName = $('#edit_last_name').val();
             const updatedNumber = $('#edit_phone').val();
             let verified = 1;
             if(updatedFirstName.trim()===''){
                $('#error-edit_name').text('Please Provide a valid first name');
                verified = -1;
             }
             if(updatedLastName.trim()===''){
                $('#error-edit_last_name').text('Please Provide a valid last name')
                verified = -1;
             }
             if (!/^\d{10}$/.test(updatedNumber.trim())) {
             $('#error-edit_phone').text('Please Provide a valid 10-digit mobile number without 0 or country code');
            verified = -1;
            }

             if(verified===-1){
                return;
             }

        fetch('/editProfile', {
            method: 'PATCH',
            headers: {
                'Content-Type': 'application/json',
            },
            body: JSON.stringify({
                updatedFirstName,updatedLastName,updatedNumber,
            }),
        })
        .then(response => response.json())
        .then(data => {
           if( data.error){
            $('#user-error-message').css('display', 'block');
           }else {
            // Handle success, update UI if needed
            $('#user-message').css('display', 'block');
            // Disable input fields
            inputFields.prop('disabled', true);

            // Revert button text to "Edit Profile"
            editProfileBtn.text('Edit Profile');

            $('#my-profile').load('/profile #my-profile')
            //Reload the area
            setTimeout(()=>{
                $('#edit-profile').load('/profile #edit-profile');
            },1000);
        }
            

        })
        .catch(error => {
            // Handle error
            console.error('Error updating profile:', error);

            // Revert button text to "Edit Profile"
            editProfileBtn.text('Edit Profile');
        });
    }
    }  
