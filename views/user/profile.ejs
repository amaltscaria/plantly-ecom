    .user-navbar a {
      color: #f2f2f2;
      text-decoration: none;
      display: inline-block;
      margin: 0 15px;
      padding: 17px;
      padding-bottom: 45px;
      transition: color 0.3s;
    }

    .delete-address-button {
      background-color: red;
      border: none;
    }

    .address-card {
      border: 1px solid #ede6e6;
    }

    .user-navbar a:hover,
    .user-navbar a.active {
      color: #ffffff;
      /* Lighter green color */
      font-size: 20px;
      transition: 1s;
    }


    .user-content {
      display: none;
      /* Initially hide all user content sections */
      padding: 21px;
      background-color: #fff;
      /* White background for user details */
      border-radius: 5px;
      box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
