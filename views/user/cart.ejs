     <script>
      $(document).ready(function () {
        // Event handling for increment button
$('#viewcart').on('click', '#incrementBtn', function () {
     // Get product ID and quantity input
    // Find the parent container of the clicked button
    const container = $(this).closest('.view-cart-d');

     // Retrieve the product ID from the data attribute
    const productId = container.data('product-id');
    const quantityInput = container.find('#quantityInput');
    const max = container.find('#quantityInput').attr('max');

    // Update the quantity input on the frontend
    let currentValue = parseInt(quantityInput.val(), 10);
    if(currentValue+1 > max){
      $('#errorHeading').text('Stock Limit Reached');
      $('#errorMessage').text('You have reached the maximum stock limit');
      $('#statusErrorsModal').modal('show');
      quantityInput.val(max);
      currentValue = +max - 1;
      
    }
    if(currentValue<1){
      $('#errorHeading').text('Invalid stock');
      $('#errorMessage').text('Please provide a valid stock number');
      $('#statusErrorsModal').modal('show');
      quantityInput.val(1);
      currentValue = 0;
    }
    if (currentValue >= 1) {
        quantityInput.val(currentValue + 1);

        // Make a fetch request to update the quantity in the database
        updateCartQuantity(productId, currentValue + 1);
    } else {
      updateCartQuantity(productId, 1);
    }
});

// Event handling for decrement button
$('#viewcart').on('click', '#decrementBtn', function () {
    // Get product ID and quantity input
    // Find the parent container of the clicked button
    const container = $(this).closest('.view-cart-d');

     // Retrieve the product ID from the data attribute
    const productId = container.data('product-id');
    const quantityInput = container.find('#quantityInput');
    const max = container.find('#quantityInput').attr('max');

    // Update the quantity input on the frontend
    let currentValue = parseInt(quantityInput.val(), 10);
    if(currentValue-1 > max){
      $('#errorHeading').text('Stock Limit Reached');
      $('#errorMessage').text('You have reached the maximum stock limit');
      $('#statusErrorsModal').modal('show');
      quantityInput.val(max);
      currentValue = +max + 1;
      
    }
    if(currentValue<1){
      $('#errorHeading').text('Invalid stock');
      $('#errorMessage').text('Please provide a valid stock number');
      $('#statusErrorsModal').modal('show');
      quantityInput.val(1);
      currentValue = 0;
    }
    if (currentValue > 1) {
        quantityInput.val(currentValue - 1);

        // Make a fetch request to update the quantity in the database
        updateCartQuantity(productId, currentValue - 1);
    } else {
      updateCartQuantity(productId, 1);
    }
});

// Function to make a fetch request to update the quantity in the database
async function updateCartQuantity(productId, newQuantity) {
   try{
    const response = await fetch('/updateCartQuantity', {
        method: 'PATCH',
        headers: {
            'Content-Type': 'application/json',
        },
        body: JSON.stringify({
            product : productId,
            newQuantity: newQuantity,
        })
    })
   if(response.status === 200){
         $('#my-profile').load('/cart #my-profile')
         $('#viewcart').load('/cart #viewcart')
         // $('#cartAmount').text('Rs. '+ +data.cartItemSum);
         // $('#cartAmount2').text('Rs. '+ +data.cartItemSum);
   }
   if(response.status === 409){
      $('#outOfStockModal').modal('show');
   }
}
   catch(err){
      console.log(err);
   }
}

      });


function deleteFromCart(id){
  $('#deleteConfirm').modal('show'); 
   $('#removeFromCart').click(()=>{
      fetch('/deleteCartItem',
  {
   method:'DELETE',
   headers:{
      'Content-Type': 'application/json',
   },
   body: JSON.stringify({id:id})
  })
  .then((response)=>{
   if(response.status===200){
      $('#deleteConfirm').modal('hide'); 
      $('#cart-del-message').css('display','block');
      $('#my-profile').load('/cart #my-profile');
      setTimeout(()=>{
      $('#viewcart').load('/cart #viewcart');
      },1000);
     
   }
  })
  .catch((err)=>{
   console.log(err);
  })
   })
  
}

$('#closeBtnStock').click(()=>{
  $('#outOfStockModal').modal('hide')
})
$('#closeX').click(()=>{
  $('#outOfStockModal').modal('hide')
})

$('#checkOutBtn').click(async ()=>{
   event.preventDefault();
   const response = await fetch('/checkCartQuantity');
   if(response.status===200){
      window.location.href = '/checkout';
   }
   if(response.status===409){
      $('#outOfStockModal2').modal('show')
   }
})

$('#closeBtnStockX').click(()=>{
  $('#outOfStockModal2').modal('hide')
})
$('#closeXX').click(()=>{
  $('#outOfStockModal2').modal('hide')
})

const movetoWish = id =>{
   $('#unlistProductModal').modal('show');
   $('#unListButton').click(()=>{
      showModal(id);
   })
}

const showModal = async (id)=>{
try{
   $('#unlistProductModal').modal('hide');
  const response1 = await fetch('/deleteCartItem',
  {
   method:'DELETE',
   headers:{
      'Content-Type': 'application/json',
   },
   body: JSON.stringify({id:id})
  })
  if(response1.status === 200){
   const response = await fetch(`/wishlist/${id}`,{
    method:'POST',
  })
  if(response.status===200){
   $('#my-profile').load('/cart #my-profile');
   $('#unListButton').off('click');
  $('#wishlistModal').modal('show');
  setTimeout(()=>{

    $('#wishlistModal').modal('hide');
    $('#viewcart').load('/cart #viewcart');
  },1000)
}

  }

 
}catch(err){
  console.log(err);
}
}
