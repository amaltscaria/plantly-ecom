  <script>
    // Disable form submissions if there are invalid fields
    (function() {
      'use strict';
      window.addEventListener('load', function() {
        // Get the forms we want to add validation styles to
        var forms = document.getElementsByClassName('needs-validation');
        // Loop over them and prevent submission
        var validation = Array.prototype.filter.call(forms, function(form) {
          form.addEventListener('submit', function(event) {
            if (form.checkValidity() === false) {
              event.preventDefault();
              event.stopPropagation();
            }
            form.classList.add('was-validated');
          }, false);
        });
      }, false);
    })();
  </script>

  <script>
    $(document).ready(function() {
      if (($("#add_new_address").is(":checked"))) {
        $('#addAddress').css('display', 'block');
        $('#check_add').collapse('show');
        $('#check_existing_address').css('display', 'none');

      }
    })
    let addressId, deliveryType, deliveryCharge, paymentMethod, selectedCouponCode, selectedCouponVal, finalAmountPay;

    addressId = $('#existing_address_1').val()
    finalAmountPay = $('#netPayable').data('net');
    if ($("#with_ship").is(":checked")) {
      deliveryType = 'express';
      deliveryCharge = 300;
    } else {
      deliveryType = 'Normal';
      deliveryCharge = 100;
    }
    function handleAddressSelection(id) {
      if (id) {
        $('#addAddress').css('display', 'none');
        addressId = id;
      } else {
        addressId = null;
        $('#addAddress').css('display', 'block');
        $('#check_add').collapse('show');
        $('#check_existing_address').collapse('hide');
      }
    }

    $('#saveAndProceedBtn').on('click', async function(e) {
      e.preventDefault();

      $('#add1').css('display', 'none');
      $('#add2').css('display', 'none');
      $('#add3').css('display', 'none');
      $('#add4').css('display', 'none');
      $('#add5').css('display', 'none');
      $('#add6').css('display', 'none');
      $('#add7').css('display', 'none');

      const name = $('#addName').val();
      const address = $('#addAddress1').val();
      const city = $('#addCity').val();
      const state = $('#addState').val();
      const country = $('#addCountry').val();
      const postCode = $('#addPost').val();
      const number = $('#addNumber').val();

      if (!validateAdd()) return;

      const response = await fetch('/addAddress', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
        },
        body: JSON.stringify({
          fullName: name,
          addressLine1: address,
          city,
          state,
          country,
          postCode,
          mobileNumber: number
        })
      })
      const x = await response.json();
      addressId = x.addedId;

      console.log(addressId);
      $('#saveAndProceedBtn').attr('disabled', true);
      $('#addForm :input').attr('disabled', true);
      $('#check_add').collapse('hide');
      $('#check_ship').collapse('show');
      $('#check_existing_address').css('display', 'none');
    });


    function validateAdd() {
      const name = $('#addName').val();
      const address = $('#addAddress1').val();
      const city = $('#addCity').val();
      const state = $('#addState').val();
      const country = $('#addCountry').val();
      const postCode = $('#addPost').val();
      const number = $('#addNumber').val();
      isValid = true;
      if (!name.trim()) {
        isValid = false;
        $('#add1').css('display', 'block');
      }
      if (!address.trim()) {
        isValid = false;
        $('#add2').css('display', 'block');
      }
      if (!city.trim()) {
        isValid = false;
        $('#add3').css('display', 'block');
      }
      if (!state.trim()) {
        isValid = false;
        $('#add4').css('display', 'block');
      }
      if (!country.trim()) {
        isValid = false;
        $('#add7').css('display', 'block');
      }
      if (!postCode.trim() || postCode.length !== 6) {
        isValid = false;
        $('#add5').css('display', 'block');
      }
      if (!number.trim() || number.length !== 10) {
        isValid = false;
        $('#add6').css('display', 'block');
      }
      return isValid;
    }

    function proceedToShipping() {
      $('#check_existing').collapse('hide');
      $('#check_ship').collapse('show');
    }

    function proceedToPayment() {
      if ($("#with_ship").is(":checked")) {
        $('#normalDelivery').css('display', 'none');
        $('#normalPay').css('display', 'none');
        $('#expressDelivery').css('display', 'block');
        $('#expressPay').css('display', 'block');
        deliveryType = 'express';
        deliveryCharge = 300;
        const amount = $('#totalValue').val();
        finalAmountPay = +amount + +deliveryCharge;
        $('#netPayable').text(+amount + deliveryCharge);
        $('#totalAmount').text(+amount + deliveryCharge);
        const selectedCouponVal = $('#coupon-select').val();
        if (selectedCouponVal !== 'default') {
          const finalAmount = amount - Math.round((selectedCouponVal / 100) * amount) + deliveryCharge;
          finalAmountPay = finalAmount;
          $('#netPayable').text(finalAmount);
        }

      } else {
        $('#expressDelivery').css('display', 'none');
        $('#expressPay').css('display', 'none');
        $('#normalDelivery').css('display', 'block');
        $('#normalPay').css('display', 'block');

        deliveryType = 'Normal';
        deliveryCharge = 100;
        const amount = $('#totalValue').val();
        finalAmountPay = +amount + +deliveryCharge;
        $('#netPayable').text(+amount + deliveryCharge);
        $('#totalAmount').text(+amount + deliveryCharge);
        const selectedCouponVal = $('#coupon-select').val();
        if (selectedCouponVal !== 'default') {
          const finalAmount = amount - Math.round((selectedCouponVal / 100) * amount) + deliveryCharge;
          finalAmountPay = finalAmount;
          $('#netPayable').text(finalAmount);
        }
      }
      $('#check_payment').collapse('show');
      $('#check_ship').collapse('hide');
    }

    $('#pay_check').submit((event) => {
      event.preventDefault();
      if (!addressId) {
        $('#errorModalHead').text('Invalid Address');
        $('#errorModalBody').text('Please select or add a valid address');
        $('#statusErrorsModal').modal('show');
        return
      }
      if ($("#cash_on_delivery").is(":checked")) {
        paymentMethod = 'Cash On Delivery';
      } else if ($("#upi_payment").is(":checked")) {
        paymentMethod = 'Online Payment'
      } else if ($("#wallet").is(":checked")) {
        paymentMethod = 'Wallet';
      }
      console.log(addressId, paymentMethod, deliveryType, selectedCouponVal);
      const walletBalance = $('#walletMoney').data('wallet')
      console.log(finalAmountPay);
      if (paymentMethod === 'Wallet' && walletBalance < finalAmountPay && walletBalance > 0) {
        $('#walletSplit').css('display', 'block');
        paymentMethod = 'Partial';
      } else $('#walletSplit').css('display', 'none');
      $('#staticBackdrop').modal('show');
    });
    $('#placeorder').click(async () => {

      try {
        let split = '';
        if (paymentMethod === 'Partial') {
          if ($('#walletCOD').is(':checked')) split = 'walletCod';
          else split = 'walletOnline'
        }

        const response = await fetch('/order', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json',
          },
          body: JSON.stringify({
            address: addressId,
            deliveryType,
            paymentMethod,
            selectedCouponCode,
            split,
          }),
        })
        if (response.status === 200) {
            const newResponse = await response.json();
            const paymentMethod = newResponse.paymentMethod;
            const orderId = newResponse.orderId;
            let amount;
            if(newResponse.online){
              amount = newResponse.online;
            }else{
              amount = newResponse.totalAmount;
            }
            const nextOrderId = newResponse.nextOrderId;
            console.log(amount);
            if(orderId){

            const options = {
              "key": 'rzp_test_VMXBmdlGP9IevY',
              "amount": amount, // Convert to the smallest currency unit
              "currency": "INR",
              "name": "Plantly Ecom",
              "description": "Ecommerce Transactions",
              "image": "/assets/img/product/product_1.jpg",
              "order_id": orderId,
              "handler": async function(response) {
                const verifySettings = {
                    method: 'POST',
                    headers: {
                      'Content-Type': 'application/json',
                    },
                    body: JSON.stringify({
                      response,
                      nextOrderId,
                      paymentMethod,
                    }),
                  };
                 const newResponse = await fetch('/api/payment/verify', verifySettings);
                 if(newResponse.status===200){
                  window.location.href = `/orderConfirmation?order=${nextOrderId}`;
                 }
              },
              "theme": {
                "color": "#99cc33"
              }
            };

            const rzp1 = new Razorpay(options);

            rzp1.on('payment.failed', function(response) {
              alert(response.error.code);
              alert(response.error.description);
              alert(response.error.source);
              alert(response.error.step);
              alert(response.error.reason);
              alert(response.error.metadata.order_id);
              alert(response.error.metadata.payment_id);
            });

            rzp1.open();
          }
        else{
          window.location.href = `/orderConfirmation?order=${nextOrderId}`;
        }
      }
      if (response.status === 409) {
        $('#errorModalHead').text('Out of Stock');
        $('#errorModalBody').text('Sorry !!! Some items went out of stock!!!');
        $('#statusErrorsModal').modal('show');
      }
    } catch (err) {
      console.log(err);
    }



    });

    $('#coupon-select').change(() => {
      if ($('#coupon-select').val() !== 'default') {
        $('#couponBtn').removeAttr('disabled');
        $('#couponBtn').text('Apply');
      } else {
        $('#couponBtn').attr('disabled', true)
        const amount = $('#totalValue').val();
        $('#coupondiv').css('display', 'none');
        selectedCouponCode = null;
        couponDiscount = 0;
        $('#discounted').text(`-${couponDiscount}`);
        $('#maximumD').text('');
        $('#discountDiv').css('color','black');
        $('#netPayable').text(+amount + +deliveryCharge);
      }


      $('#couponBtn').click(() => {
        console.log(deliveryCharge);
        selectedCouponCode = $('#coupon-select').find(':selected').data('code');
        const maximumDiscount = $('#coupon-select').find(':selected').data('max');

        if ($('#coupon-select').val() !== 'default') {
          selectedCouponVal = $('#coupon-select').val();
          $('#coupondiv').css('display', 'block');
          $('#couponSpan').text(selectedCouponVal);
          const amount = $('#totalValue').val();
          let couponDiscount = Math.floor((selectedCouponVal / 100) * amount);
          if(couponDiscount>maximumDiscount)couponDiscount = maximumDiscount
          const finalAmount = amount - couponDiscount;
          $('#discounted').text(`-${couponDiscount}`);
          $('#maximumD').text(`(Max Discount ${maximumDiscount})`);
          $('#discountDiv').css('color','red');
          $('#totalAmount1').text(`Rs. ${finalAmount}`);
          $('#netPayable').text(finalAmount + deliveryCharge);
          $('#couponBtn').text('Applied');
          $('#couponBtn').attr('disabled', true)
        }
        $('#couponBtn').off('click');
      })
    })

    $('#showAllCouponsBtn').click(()=>{
      $('#exampleModalLong').modal('show');
    })

    $('#closeCoupon').click(()=>{
      $('#exampleModalLong').modal('hide');
    })
    $('#couponCloseX').click(()=>{
      $('#exampleModalLong').modal('hide');
    })
  </script>




